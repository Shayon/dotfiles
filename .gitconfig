[user]
	name = Andrew Seward
	email = awswrd@gmail.com
[alias]
  st = status
  ci = commit
  co = checkout
  br = branch
  cl = clean -df
  di = !echo "Deprecated that one: Use git files"
  files = diff --name-only
  chart = !git diff --stat=$(tput cols)
  summary = diff --shortstat
  sub = !git submodule update --init --recursive
  mine = !git branch -a | grep -isn --color "[^remotes/origin]dev/as_"
  pop = stash apply stash@{0}
  info = !clear && git status && git stash list
  local = branch -l
  back = checkout -
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  autocrlf = input
[diff]
	algorithm = patience
